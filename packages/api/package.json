{
  "name": "@pdeals/api",
  "version": "0.0.1",
  "description": "PokerDeals API",
  "license": "Unlicense",
  "scripts": {
    "dev": "./node_modules/.bin/ts-node-dev src/index.ts",
    "tsc": "tsc",
    "start": "npx ts-node src/index.ts",
    "start:in:docker": "node dist/index.js",
    "start:cluster": "./node_modules/.bin/ts-node src/cluster.ts",
    "build": "npm run clean && tsc && npm run swagger",
    "clean": "rimraf dist",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "test-dev": "cross-env NODE_ENV=test mocha --exit",
    "test:coverage": "nyc npm run test",
    "swagger": "swaggerGen -c ./swagger.config.yml",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint": "eslint -c ../frontend-next/.eslintrc . --ext .ts",
    "regen-schemas": "ts-node generateSchemas.ts",
    "seed:run": "env-cmd ts-node ../../node_modules/typeorm-seeding/dist/cli.js seed -r $(pwd)/../..",
    "importdata": "env-cmd ./node_modules/.bin/ts-node-dev src/import.ts",
    "importreviews": "env-cmd ./node_modules/.bin/ts-node-dev src/import-reviews.ts"
  },
  "dependencies": {
    "@pdeals/db": "*",
    "@pdeals/models": "*",
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "@types/bull-board": "^0.6.0",
    "@types/csv-parse": "^1.2.2",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/mkdirp": "^1.0.1",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "ajv": "^6.12.3",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.19.1",
    "bull-board": "^0.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.4",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "fp-ts": "^2.7.0",
    "gmail-send": "^1.8.10",
    "google-spreadsheet": "^3.1.15",
    "io-ts": "^2.2.9",
    "json2csv": "^5.0.5",
    "jsonschema": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "memory-cache": "^0.2.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "5.7.5",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.18",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.29",
    "typeorm-seeding": "^1.6.1",
    "typescript-ioc": "^1.2.5",
    "typescript-rest": "^3.0.1",
    "typescript-rest-swagger": "^1.0.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "4.1.7",
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.159",
    "@types/memory-cache": "^0.2.1",
    "@types/mocha": "5.2.5",
    "@types/mongodb": "3.1.27",
    "@types/mongoose": "5.5.3",
    "@types/morgan": "1.7.35",
    "@types/node-fetch": "^2.5.7",
    "@types/passport-jwt": "^3.0.1",
    "@types/request": "2.48.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "eslint": "^7.2.0",
    "mocha": "5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "request": "2.87.0",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "5.17.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.5.1",
    "typescript-json-schema": "^0.42.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "./reports/coverage",
    "sourceMap": true,
    "instrument": true
  }
}
